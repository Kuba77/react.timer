{
  "name": "react.timer",
  "version": "2.3.1",
  "description": "A super simple timer component made in ReactJS",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development&& hjs-dev-server",
    "build": "set NODE_ENV=production&& babel src --out-dir lib",
    "test": "set NODE_ENV=test&& ava --tap | tap-dot",
    "test:watch": "set NODE_ENV=test&& ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogermarkussen/react.timer.git"
  },
  "keywords": [
    "timer",
    "react",
    "webpack",
    "babel",
    "es6",
    "es7"
  ],
  "author": "Roger Markussen <roger.markussen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rogermarkussen/react.timer/issues"
  },
  "homepage": "https://github.com/rogermarkussen/react.timer#readme",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "hjs-webpack": "^7.2.1",
    "react-addons-test-utils": "^0.14.7",
    "tap-dot": "^1.0.4",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.{js,jsx}"
    ],
    "failFast": true,
    "tap": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
